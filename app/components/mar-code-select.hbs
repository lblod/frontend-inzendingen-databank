<div class="ember-power-select--small" {{did-update this.updateSelectedValue @value}}>
  <PowerSelectMultiple @placeholder="MAR7300, MAR7304, ..." @search={{perform this.search}} @searchPlaceholder="Typ om te zoeken..." @searchEnabled={{true}} @options={{this.options}} @selected={{this.selected}} @onChange={{this.changeSelected}} @allowClear={{true}} as |marCode|>
    {{marCode.label}}
  </PowerSelectMultiple>
</div>