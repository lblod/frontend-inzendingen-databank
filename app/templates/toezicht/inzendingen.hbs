{{#if this.currentSession.canReadVlabel}}
  <Inzendingen::Vlabel
    @model={{this.model}}
    @isLoading={{this.isLoadingModel}}
    @displaySubset={{this.hasActiveChildRoute}}
    @filter={{hash
                bestuurseenheidIds=this.bestuurseenheidIds
                marCodeIds=this.marCodeIds
                sessionDateFrom=this.sessionDateFrom
                sessionDateTo=this.sessionDateTo
                sentDateFrom=this.sentDateFrom
                sentDateTo=this.sentDateTo
                dateOfEntryIntoForceFrom=this.dateOfEntryIntoForceFrom
                dateOfEntryIntoForceTo=this.dateOfEntryIntoForceTo
                endDateFrom=this.endDateFrom
                endDateTo=this.endDateTo
            }}
    @page={{this.page}}
    @size={{this.size}}
    @sort={{this.sort}}
    @onFilterChange={{this.updateQueryParams}}
  />
{{else}}
  <Inzendingen::Regular
    @model={{this.model}}
    @isLoading={{this.isLoadingModel}}
    @displaySubset={{this.hasActiveChildRoute}}
    @filter={{hash
                bestuurseenheidIds=this.bestuurseenheidIds
                classificatieIds=this.classificatieIds
                provincieIds=this.provincieIds
                besluitTypeIds=this.besluitTypeIds
                regulationTypeId=this.regulationTypeId
                sessionDateFrom=this.sessionDateFrom
                sessionDateTo=this.sessionDateTo
                sentDateFrom=this.sentDateFrom
                sentDateTo=this.sentDateTo
                statusUri=this.statusUri
            }}
    @page={{this.page}}
    @size={{this.size}}
    @sort={{this.sort}}
    @onFilterChange={{this.updateQueryParams}}
  />
{{/if}}
{{outlet}}